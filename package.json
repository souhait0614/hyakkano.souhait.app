{
  "name": "hyakkano.souhait.app",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "waku dev",
    "build": "waku build",
    "start": "waku start",
    "deploy": "pnpm build --with-cloudflare && wrangler deploy",
    "lint": "concurrently --pad-prefix node:lint:*",
    "lint:typescript": "tsc --noEmit",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint .",
    "format": "eslint --fix . && stylelint --fix ."
  },
  "dependencies": {
    "@date-fns/tz": "1.4.1",
    "@tanstack/react-table": "8.21.3",
    "canvas-confetti": "1.9.4",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-server-dom-webpack": "19.2.4",
    "schummar-translate": "1.26.1",
    "tailwind-merge": "3.4.0",
    "tailwind-variants": "3.2.2",
    "valibot": "1.2.0",
    "waku": "0.27.5"
  },
  "devDependencies": {
    "@stylistic/stylelint-config": "4.0.0",
    "@tailwindcss/vite": "4.1.18",
    "@taiyme/eslint-config": "1.0.0",
    "@taiyme/vite-plugin-jsx-prune-classname": "0.0.3",
    "@types/canvas-confetti": "1.9.0",
    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/parser": "8.55.0",
    "concurrently": "9.2.1",
    "eslint": "10.0.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-plugin-better-tailwindcss": "4.2.0",
    "globals": "17.3.0",
    "kiso.css": "1.2.4",
    "stylelint": "17.2.0",
    "stylelint-config-recess-order": "7.6.1",
    "stylelint-config-recommended": "18.0.0",
    "stylelint-config-tailwindcss": "1.0.1",
    "tailwindcss": "4.1.18",
    "terser": "5.46.0",
    "type-fest": "5.4.4",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.1.1"
  },
  "packageManager": "pnpm@10.29.3",
  "pnpm": {
    "overrides": {
      "vite": "$vite"
    },
    "patchedDependencies": {
      "waku": "patches/waku.patch"
    }
  }
